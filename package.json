{
  "name": "my-electron-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && npx electron .\"",
    "electron:build": "npm run build && electron-builder",
    "start": "npx electron ."
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@tanstack/react-query": "^5.66.9",
    "electron-is-dev": "^3.0.1",
    "lucide-react": "^0.475.0",
    "papaparse": "^5.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.3.0",
    "electron-builder": "^25.1.8",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "wait-on": "^8.0.2"
  },
  "build": {
    "appId": "com.yourcompany.dashboard",
    "productName": "Vehicle Dashboard",
    "files": [
      "dist/**/*",
      "src/main/**/*.js",
      "index.js"
    ],
    "directories": {
      "buildResources": "src/assets",
      "output": "release"
    },
    "extraResources": [
      {
        "from": "backend",
        "to": "backend",
        "filter": ["**/*"]
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/assets/icon.ico"
    }
  }
}